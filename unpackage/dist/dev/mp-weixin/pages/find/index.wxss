image {
  border-radius: 10rpx;
}
.find {
  padding-bottom: 31rpx;
}
.find .moregame {
    margin-top: 20rpx;
}
.find .moregame .enter {
      margin-left: 15rpx;
      margin-right: 42rpx;
      color: #999999;
      font-size: 21rpx;
      -webkit-transform: rotate(-90deg);
              transform: rotate(-90deg);
}
.find .moregame .entertext {
      font-size: 42rpx;
      color: #343434;
      margin-left: 41rpx;
      font-weight: bold;
      margin-bottom: 10rpx;
}
.find .moregame .more {
      font-size: 24rpx;
      color: #999999;
      margin-left: 15rpx;
}
.find .gamefun {
    width: 100%;
    white-space: nowrap;
    margin-left: 20rpx;
    border-radius: 10px;
}
.find .gamefun .fun {
      width: 570rpx;
      height: 221rpx;
      margin-right: 11rpx;
      display: -webkit-inline-box;
      display: -webkit-inline-flex;
      display: inline-flex;
}
.find .gamefun .game {
      width: 230rpx;
      margin-right: 30rpx;
      display: inline-block;
      text-align: center;
}
.find .gamefun .game .new {
        height: 170rpx;
        width: 100%;
}
.find .gamefun .gametext {
      font-size: 26rpx;
      color: #343434;
}

