page {
  background: #F0F0F0;
}
.head {
  height: 120rpx;
  background: #FD5E2D;
}
.head .order {
    width: 140rpx;
    height: 105rpx;
}
.head .pay {
    font-size: 48rpx;
    color: #FFFFFF;
    margin-left: 20rpx;
    font-weight: bold;
}
.topbar {
  background: white;
  padding: 15px 0;
}
.topbar .space {
    margin-bottom: 60rpx;
}
.logistics {
  margin: 0 23rpx;
}
.logistics .icon, .logistics .icon2 {
    border-radius: 50%;
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin-right: 20rpx;
    color: #FFFFFF;
}
.logistics .icon {
    background: -webkit-linear-gradient(bottom, #2d76fd, #2d8ffd);
    background: linear-gradient(0deg, #2d76fd, #2d8ffd);
}
.logistics .icon .car {
      font-size: 24rpx;
}
.logistics .icon2 {
    background: -webkit-linear-gradient(bottom, #fd5e2d, #fd852d);
    background: linear-gradient(0deg, #fd5e2d, #fd852d);
}
.logistics .icon2 .position {
      font-size: 28rpx;
}
.logistics .express {
    line-height: 1.2;
    color: #4893F1;
    font-size: 29rpx;
    margin-bottom: 18rpx;
}
.logistics .date {
    color: #999999;
    font-size: 16rpx;
}
.logistics .enter {
    font-size: 28rpx;
    color: #999999;
    margin-left: 55rpx;
}
.info {
  color: #343434;
}
.info .nick {
    font-size: 26rpx;
}
.info .mobile {
    color: #999999;
    font-size: 26rpx;
    margin-left: 40rpx;
}
.info .address {
    margin-top: 18rpx;
    font-size: 24rpx;
    line-height: 1.2;
}
.orders {
  margin-top: 20rpx;
  background: #FFFFFF;
}
.orders .order {
    padding-bottom: 42rpx;
    background: #FFFFFF;
}
.orders .order .gamecover {
      width: 120rpx;
      height: 120rpx;
      border-radius: 5rpx;
      margin: 20rpx 26rpx 0 22rpx;
}
.orders .order .gamecontent .gametitle {
      color: #343434;
      font-size: 34rpx;
      margin-top: 10rpx;
      font-weight: bold;
}
.orders .order .gamecontent .desc {
      color: #666666;
      font-size: 24rpx;
}
.orders .order .gamecontent .subdesc {
      color: #999999;
      font-size: 18rpx;
}
.orders .order .gamecontent .price .pricecount {
      font-size: 28rpx;
      color: #FD5E2D;
      margin-right: 306rpx;
}
.orders .order .gamecontent .sum {
      font-size: 26rpx;
      color: #343434;
      margin-right: 20rpx;
      margin-bottom: 30rpx;
}
.orders .order .gamecontent .del, .orders .order .gamecontent .confirm {
      width: 130rpx;
      height: 40rpx;
      border-radius: 20rpx;
}
.orders .order .gamecontent .del {
      border: 1px solid #DDDDDD;
}
.orders .order .gamecontent .del .deltext {
        color: #666666;
        font-size: 19rpx;
}
.orders .order .gamecontent .confirm {
      border: 1px solid #FD5E2D;
      margin: 0 20rpx 0 22rpx;
}
.orders .order .gamecontent .confirm .confirmtext {
        color: #FD5E2D;
        font-size: 19rpx;
}
.realPay {
  border-top: 1px solid #DDDDDD;
  background: white;
  height: 60rpx;
  padding: 0 23rpx;
}
.realPay .total {
    font-size: 34rpx;
    color: #343434;
    font-weight: bold;
}
.realPay .active {
    font-size: 28rpx;
    color: #FD5E2D;
}
.settle {
  background: #FFFFFF;
  margin-top: -22rpx;
  padding: 0 22rpx;
  color: #666666;
}
.settle view {
    line-height: 2;
}
.settle .lefttext {
    font-size: 28rpx;
    color: #666666;
}
.settle .righttext {
    font-size: 28rpx;
}
.settle .total {
    font-size: 34rpx;
    color: #343434;
    font-weight: bold;
}
.settle .totalcount {
    font-size: 28rpx;
    color: #343434;
}
.settle .active {
    font-size: 19rpx;
    color: #FD5E2D;
}
.orderinfo {
  margin-top: 19rpx;
  background: #FFFFFF;
  padding: 20rpx 0;
  padding-left: 23rpx;
}
.orderinfo view {
    line-height: 2;
}
.orderinfo .title {
    font-size: 34rpx;
    color: #343434;
    font-weight: bold;
}
.orderinfo .base {
    width: 188rpx;
    font-size: 28rpx;
    color: #666666;
}
.orderinfo .baseright {
    color: #666666;
    font-size: 28rpx;
}
.apply {
  height: 90rpx;
  background: white;
  box-shadow: 0px -1px 0px 0px #dddddd;
}
.apply .btnapply {
    width: 130rpx;
    height: 40rpx;
    background: white;
    border: 1px solid #dddddd;
    border-radius: 20rpx;
}
.apply .space {
    margin: 0 21rpx;
}
.apply .applytext {
    color: #666666;
    font-size: 19rpx;
}

